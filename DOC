//CAPTURAMOS EL LOG DE APACHE EN LOS CONTENEDORES
docker logs -f [containerName] >/dev/null   

//ENTRAT AL CONTENEDOR

docker exec -it <mycontainer> bash


//////correr mencache en el contenedor /////

/etc/init.d/memcached start



